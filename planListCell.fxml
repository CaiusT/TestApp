<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="planPane" minHeight="60.0" prefHeight="60.0" prefWidth="400.0" style="-fx-background-color: #FFFFFF; -fx-background-radius: 10; -fx-border-color: #98CA48; -fx-border-radius: 10;" xmlns="http://javafx.com/javafx/8.0.321" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <Label fx:id="planNumber" text="01" BorderPane.alignment="CENTER">
         <font>
            <Font name="Tahoma Bold" size="16.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </Label>
   </left>
   <center>
      <Label fx:id="planName" text="Flowchart" BorderPane.alignment="CENTER">
         <font>
            <Font name="Tahoma Bold" size="16.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </Label>
   </center>
</BorderPane>
